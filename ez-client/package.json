  },
  "scripts": {
    "test": "node ./node_modules/karma/bin/karma start --single-run ",
    "serve": "NODE_ENV=dev node_modules/.bin/webpack-dev-server --progress --port 8081",
    "api": "babel-node --presets es2015 ./node_modules/.bin/json-server api/db.js --routes api/routes.json --port 8080",
    "package": "NODE_ENV=prod webpack -p --progress",
    "deploy": "mkdir -p ../ez-server/target/classes/static && cp -R dist/* ../ez-server/target/classes/static"
  },
  "keywords": [
    "free",
    "dashboard",
    "ez-dashing",
